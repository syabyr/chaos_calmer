config ipcam 'global'
	option vendor	'jvt'
	option model	's130h18v'
	option soc	'hi3518ev200'
	option totmem	'64M'
	option sysmem	'40M'
	option flash	'8M'

config ipcam 'video'
	option sensor	'ar0130'
	option disabled	'0'

config ipcam 'gpio'
	option ircut1	'64'
	option ircut2	'65'
	option light	'47'
	option reset	'0'

config ipcam 'i2c'
	option status	'-'
